APP_OBJ=$(BUILD_DIR)/main
VOLUME_OBJ=$(BUILD_DIR)/volume.o
OPERATION_OBJ=$(BUILD_DIR)/operation.o
VOLUME=volume
OPERATION=operation
MUL_LIB=mul
OBJS+=$(VOLUME_OBJ)
OBJS+=$(OPERATION_OBJ)
LIBS+=$(MUL_LIB)
	

.PHONY: $(APP)
$(APP):
	$(MAKE) $(VOLUME_OBJ) -C $(VOLUME)
	$(MAKE) $(OPERATION_OBJ) -C $(OPERATION)
	$(MAKE) $(APP_OBJ) -C $(APP)
