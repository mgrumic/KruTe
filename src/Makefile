APP_OBJ=$(BUILD_DIR)/main
VOLUME_OBJ=$(BUILD_DIR)/volume.o
OPERATION_OBJ=$(BUILD_DIR)/operation.o
FORCE_OBJ=$(BUILD_DIR)/force.o
VOLUME=volume
FORCE=force
OPERATION=operation
MUL_LIB=mul
DIV_LIB=div
OBJS+=$(VOLUME_OBJ)
OBJS+=$(OPERATION_OBJ)
OBJS+=$(FORCE_OBJ)

LIBS+=$(MUL_LIB)
LIBS+=$(DIV_LIB)

SUB_OBJ=$(BUILD_DIR)/sub.o
SUB=sub
OBJS+=$(SUB_OBJ)

.PHONY: $(APP)
$(APP):
	$(MAKE) $(VOLUME_OBJ) -C $(VOLUME)
	$(MAKE) $(OPERATION_OBJ) -C $(OPERATION)
	$(MAKE) $(FORCE_OBJ) -C $(FORCE)
	$(MAKE) $(SUB_OBJ) -C $(SUB)
	$(MAKE) $(APP_OBJ) -C $(APP)
